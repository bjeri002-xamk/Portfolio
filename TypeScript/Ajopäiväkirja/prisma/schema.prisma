generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

//npx prisma generate aina jos muutoksia
model Ajotieto {
  id         Int    @id @default(autoincrement())
  reitti     String
  km         Int
  kayttajaId Int
  date       String
}
